# Server Configuration
SHUDL_SERVER_HOST=0.0.0.0
SHUDL_SERVER_PORT=8080
SHUDL_SERVER_READ_TIMEOUT=30
SHUDL_SERVER_WRITE_TIMEOUT=30
SHUDL_SERVER_ENVIRONMENT=development

# Logger Configuration
SHUDL_LOGGER_LEVEL=info
SHUDL_LOGGER_ENVIRONMENT=development
SHUDL_LOGGER_TIME_FORMAT=15:04:05

# Docker Configuration
SHUDL_DOCKER_COMPOSE_FILE=docker-compose.yml
SHUDL_DOCKER_PROJECT_NAME=shudl
SHUDL_DOCKER_DEFAULT_REGISTRY=
SHUDL_DOCKER_TIMEOUT=300

# Docker Environment Variables (optional)
# DOCKER_HOST=unix:///var/run/docker.sock 